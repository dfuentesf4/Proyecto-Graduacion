<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <Color x:Key="Primary">#244F75</Color>
    <Color x:Key="LightPrimary">#2874A6</Color>
    <Color x:Key="Black">#000000</Color>
    <Color x:Key="White">#FFFFFF</Color>
    <Color x:Key="LightBackground">#E5E5E5</Color>
    <Color x:Key="DarkBackground">#171717</Color>
    <Color x:Key="AcceptGreen">#00A600</Color>
    <Color x:Key="CancelRed">#FF0000</Color>
    <Color x:Key="LightFont">#e6e6e6</Color>
    <Color x:Key="Silver">Silver</Color>

    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource Primary}"/>

</ResourceDictionary>